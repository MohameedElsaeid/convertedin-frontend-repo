    "sidebar-component:build:prod": "nx run sidebar-component:build:production --output-hashing none",
    "sidebar-component:build:analyze": "nx run sidebar-component:build:stage --output-hashing none --statsJson",
    "sidebar-component:analyze": "webpack-bundle-analyzer dist/apps/sidebar-component/stats.json",
    "super-admin:serve": "nx serve super-admin -o --port 4300",
    "super-admin:build:staging": "nx run super-admin:build:stage",
    "super-admin:build:prod": "nx run super-admin:build:production",
    "super-admin:build:analyze": "nx run super-admin:build:stage --statsJson",
    "super-admin:analyze": "webpack-bundle-analyzer dist/apps/super-admin/stats.json",
