  "scripts": {
    "redmose:serve": "nx serve redmose -o --port 4200",
    "redmose:build:prod": "nx run redmose:build:production --deploy-url app-assets/ && bash build.sh",
    "redmose:build:staging": "nx run redmose:build:stage --skip-nx-cache --deploy-url app-assets/ && bash build.sh",
    "redmose:build:dev": "nx run redmose:build:stage --skip-nx-cache --deploy-url app-assets/ && bash build.sh",
    "redmose:build:analyze": "nx run redmose:build:stage --skip-nx-cache --deploy-url app-assets/ --statsJson",
    "redmose:analyze": "webpack-bundle-analyzer dist/apps/redmose/app-assets/stats.json",
    "flyerz:serve": "nx serve flyerz -o --port 4200",
    "flyerz:build:staging": "nx run flyerz:build:stage",
    "flyerz:build:prod": "nx run flyerz:build:production",
    "flyerz:build:analyze": "nx run flyerz:build:stage --statsJson",
    "flyerz:analyze": "webpack-bundle-analyzer dist/apps/flyerz/stats.json",
