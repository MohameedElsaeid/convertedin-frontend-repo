    "converted-pay:build:staging": "nx run converted-pay:build:stage && node apps/converted-pay/optimize-css.js",
    "converted-pay:build:dev": "nx run converted-pay:build:dev && node apps/converted-pay/optimize-css.js",
    "converted-pay:build:prod": "nx run converted-pay:build:production && node apps/converted-pay/optimize-css.js",
    "converted-pay:build:analyze": "nx run converted-pay:build:stage --statsJson",
