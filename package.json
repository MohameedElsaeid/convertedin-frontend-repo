    "roi-calculator:build:prod": "nx run: roi-calculator:build:production",
    "roi-calculator:build:analyze": "nx run roi-calculator:build:dev --statsJson",
    "roi-calculator:analyze": "webpack-bundle-analyzer dist/apps/roi-calculator/stats.json",
    "start": "npm run redmose:serve"
